<%-include('partials/header')-%>

<form action="/DB" method="POST">
    <label>Shoe Name</label>
    <input name = "modelname" type="text" class="input-shoe-name">
    <label>Shoe Price</label>
    <input  name = "price" type="text" class="input-shoe-Price">
    <label for="shoe category">category</label>
    <input type="text" class="input-category" name="category">
    <label>Shoe link display</label>
    <input  name = "displaylink" type="text" class="input-shoe-link-display">
    <label>Shoe link cart</label>
    <input  name = "cartlink"type="text" class="input-shoe-link-cart">
    <label for="brand">Brand</label>
    <input type="text" class="input-shoe-brand" name="brand">
    <label for="productId">productId</label>
    <input type="text" class="input-category" name="productId">
    <label for="size">size</label>
    <input type="text" class="input-category" name="size">
    <button>Submit</button>
</form>
